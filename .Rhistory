# Quick glimpse to check
glimpse(df_all)
# Make sure dpoy is numeric binary 0/1
df_all <- df_all %>%
mutate(dpoy = as.numeric(dpoy))
# Select features and target
model_data <- df_all %>%
select(dpoy, `STL%`, `BLK%`, DWS, DBPM) %>%
drop_na()  # Remove rows with missing values
# Train logistic regression model
dpoy_model <- glm(dpoy ~ ., data = model_data, family = binomial)
# Summary of the model
summary(dpoy_model)
# Predict probabilities on the dataset (optional)
model_data <- model_data %>%
mutate(dpoy_prob = predict(dpoy_model, type = "response"))
# View predictions
head(model_data %>% arrange(desc(dpoy_prob)))
